    className={`${classes.control} ${\n            emailState.isValid === false ? classes.invalid : \"\"\n          }`}\n        >\n          <label htmlFor=\"email\">E-Mail</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            value={emailState.value}\n            onChange={emailChangeHandler}\n            onBlur={validateEmailHandler}\n          />\n        </div>\n        <div\n          className={`${classes.control} ${\n            passwordState.isValid === false ? classes.invalid : \"\"\n          }`}\n        >\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={passwordState.value}\n            onChange={passwordChangeHandler}\n            onBlur={validatePasswordHandler}\n          />\n        </div>\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","/Users/jiantingzhi/Desktop/patrick/study-react/lesson-9/src/components/Home/Home.js",[],"/Users/jiantingzhi/Desktop/patrick/study-react/lesson-9/src/components/MainHeader/MainHeader.js",[],"/Users/jiantingzhi/Desktop/patrick/study-react/lesson-9/src/components/MainHeader/Navigation.js",[],"/Users/jiantingzhi/Desktop/patrick/study-react/lesson-9/src/components/UI/Button/Button.js",[],"/Users/jiantingzhi/Desktop/patrick/study-react/lesson-9/src/components/UI/Card/Card.js",[],"/Users/jiantingzhi/Desktop/patrick/study-react/lesson-9/src/components/store/auto-context.js",[],{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","severity":1,"message":"48","line":72,"column":6,"nodeType":"49","endLine":72,"endColumn":37,"suggestions":"50"},"no-native-reassign",["51"],"no-negated-in-lhs",["52"],"react-hooks/exhaustive-deps","React Hook useEffect has missing